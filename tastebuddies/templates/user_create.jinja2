{% extends "base.jinja2" %}

{% block body %}

<h1>You want to sign up for an account!</h1>

<form action="{{ request.route_url('verify') }}">
    <input type="text" name="username" id="username"
    value="Username"/>

    <input type="text" name="password" id="password"
    value="Password"/>

    <input type="submit" name="submit" value="Create Account"/>
</form>

{% endblock %}