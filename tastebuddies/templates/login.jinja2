{% extends "base.jinja2" %}

{% block body %}

<h1>Log In!</h1>

<div id="#form-container">
<form id="login-form" action="{{ request.route_url('user_login') }}" method='POST'>
    <input class="input" type="text" name="username" id="username"
    placeholder="Username"/>
    <input class="input" type="text" name="password" id="password"
    placeholder="Password"/>
    <input id="login" type="submit" name="submit" value="Login"/>
</form>
</div>

{% endblock %}
